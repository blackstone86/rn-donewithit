{
  "name": "donewithit",
  "version": "1.0.0",
  "description": "A marketplace for selling the stuff you don't need anymore.",
  "author": "Junwen Huang",
  "scripts": {
    "start": "expo start",
    "start:eject": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "commit": "git-cz",
    "format": "prettier --write .",
    "test": "jest",
    "postinstall": "husky install",
    "optimize": "npx expo-optimize",
    "bugsnag:init": "node ./node_modules/bugsnag-expo-cli/cli.js init",
    "publish:staging": "expo publish --release-channel staging",
    "publish:prod": "expo publish --release-channel prod",
    "build:android": "eas build -p android",
    "upload:android": "eas submit -p android --latest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/hooks": "^2.8.1",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.2",
    "apisauce": "^2.1.5",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-blur": "~11.0.0",
    "expo-constants": "~13.0.1",
    "expo-crypto": "~10.1.1",
    "expo-device": "~4.1.0",
    "expo-image-picker": "~12.0.1",
    "expo-location": "~14.0.1",
    "expo-media-library": "~14.0.0",
    "expo-notifications": "~0.14.0",
    "expo-secure-store": "~11.1.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.7",
    "formik": "^2.2.9",
    "immer": "^9.0.12",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "jwt-decode": "^3.1.2",
    "lottie-react-native": "5.0.1",
    "mime": "^3.0.0",
    "moment": "^2.29.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-expo-image-cache": "^4.1.0",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@bugsnag/expo": "^44.0.0",
    "@react-native-community/netinfo": "7.1.3",
    "@types/jest": "^27.5.1",
    "@types/mime": "^2.0.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.23.0",
    "bugsnag-expo-cli": "^44.0.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "latest",
    "git-cz": "^4.8.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "17",
    "sharp-cli": "^1.15.0",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "17.0.39"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|yup/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "private": true,
  "packageManager": "yarn@3.2.0"
}
